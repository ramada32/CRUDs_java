

public class Util{

     
     public Util(){
     
     }
     
     
     public static void say(){
     say("",true);
     
     }
     public static void say(String s){
     
     say(s,true);
     
     }
     public static void say(String s,Boolean tes){
     if(tes == true){
     System.out.println(s);
     }
     
     else{
     System.out.print(s);
     }
     
    }
  
  
  
  
  }
