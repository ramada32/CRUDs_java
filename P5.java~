//P5 JAVA

public class P5{

     public static void main(String[]args){

        int i = 1;
        int e = 2;
        int resultado;

         resultado = i + e;

      System.out.println("resultado soma = " +resultado);

         resultado ++;

      System.out.println("resultado incremento = " +resultado);


         resultado = resultado - e;

       System.out.println("resultado subtracao= " +resultado);

         resultado = 10 / 2;

       System.out.println("resultado divisao= " +resultado);
       
          resultado = resultado * 3;
          
       System.out.println("resultado multiplicacao= " +resultado);
       
       resultado --;
          
       System.out.println("resultado decremento= " +resultado);
       
       resultado = 7 % 2;
          
       System.out.println("resultado resto= " +resultado);
       
       }
}
       
       