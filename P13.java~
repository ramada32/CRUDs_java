//P13 java

public class P13{

  public static void main(String[]args){
  	  
  	  
  	  System.out.println("argumentos = " + args.length);
  	  
  	  
  	  if(args.length > 0 ){
  	  
  	  System.out.println("argumentos = " + args[0]);
     }
     else{
     System.out.println("nenhum parametro foi enviado!");	 
     	 
     }
  }
}